{
  "name": "Disable Ctrl+S",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Disable Ctrl+S in browser extension for Chromium and Firefox",
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/icon500.png",
    "48": "icons/icon500.png",
    "128": "icons/icon500.png"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon500.png",
      "48": "icons/icon500.png",
      "128": "icons/icon500.png"
    }
  },
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content/content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "options_page": "options/options.html",
  "browser_specific_settings": {
    "gecko": {
      "id": "disable-ctrl-s-browser@ntvu19.dev",
      "strict_min_version": "109.0"
    }
  }
}